{
	"apiVersion": "apps/v1",
	"kind": "Deployment",
	"metadata": {
		"name": "mike-deploy7"
	},
	"spec": {
		"selector": {
			"matchLabels": {
				"app": "nginx"
			}
		},
		"replicas": 2,
		"template": {
			"metadata": {
				"labels": {
					"app": "nginx"
				}
			},
			"spec": {
				"containers": [
					{
						"name": "nginx",
						"image": "nginx:1.18"
					}
				]
			}
		}
	}
}
